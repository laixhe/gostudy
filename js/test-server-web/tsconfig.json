{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./",
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    //"paths": {"@/*":["src/*"]}
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
